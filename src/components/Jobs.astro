---
/**
 * Gets posts from https://wpzlecenia.pl/wp-json/wp/v2/posts and displays them
 */

const posts = await fetch('https://wpzlecenia.pl/wp-json/wp/v2/posts');
const postsData = await posts.json();
---

<div>
    <ul>
        {postsData.map((post) => (
            <li>
                <h2>{post.title.rendered}</h2>
            </li>
        ))}
    </ul>
</div>